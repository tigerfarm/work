{
  "name": "twilio-chat",
  "version": "5.0.1",
  "description": "Twilio Chat service client library",
  "main": "./dist/lib.js",
  "browser": "./dist/browser.js",
  "types": "./dist/lib.d.ts",
  "author": "Twilio",
  "license": "MIT",
  "files": [
    "dist/docs",
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf ./coverage ./dist",
    "lint": "npx tslint -p ./tslint.json",
    "test:unit": "env FORCE_COLOR=1 NODE_ENV=test npx nyc --report-dir coverage/unit ./node_modules/mocha/bin/mocha test/unit/index.ts",
    "test:integration": "env FORCE_COLOR=1 NODE_ENV=test npx nyc --report-dir coverage/integration ./node_modules/mocha/bin/mocha test/integration/index.ts",
    "build": "npx rollup -c",
    "sourceMapReport:html": "npx source-map-explorer@2.5.2 ./dist/lib.js --html ./dist/lib.report.html",
    "sourceMapReport:json": "npx source-map-explorer@2.5.2 ./dist/lib.js --json ./dist/lib.report.json",
    "docs": "npx jsdoc@3.5.5 -c ./jsdoc.json && npx ts-node docs-postprocess.ts",
    "ci": "yarn clean && yarn lint && yarn build && yarn sourceMapReport:html && yarn sourceMapReport:json && yarn docs"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.5",
    "twilio-mcs-client": "0.4.1",
    "twilio-notifications": "^0.5.11",
    "twilio-sdk-type-validator": "^0.1.3",
    "twilio-sync": "^1.0.0",
    "twilsock": "^0.6.1",
    "iso8601-duration": "=1.2.0",
    "core-js": "^3.14.0",
    "loglevel": "^1.6.6",
    "operation-retrier": "^4.0.1",
    "platform": "^1.3.6",
    "rfc6902": "^3.0.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/chai": "^4.2.5",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-string": "^1.4.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.12",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.2",
    "async": "^3.0.1",
    "async-test-tools": "^1.0.7",
    "backoff": "^2.5.0",
    "better-docs": "^2.3.2",
    "browserslist": "^4.13.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.2.0",
    "del": "^5.1.0",
    "fancy-log": "^1.3.3",
    "fs": "0.0.2",
    "glob": "^7.1.7",
    "ink-docstrap": "^1.3.2",
    "isomorphic-form-data": "^2.0.0",
    "jsdoc": "~3.5.5",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^6.2.2",
    "mocha.parallel": "^0.15.6",
    "node-fetch": "^2.6.1",
    "nyc": "^14.1.1",
    "path": "^0.12.7",
    "rollup": "^2.51.1",
    "rollup-plugin-polyfill-node": "^0.6.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "twilio": "^3.37.1",
    "typescript": "^4.3.2",
    "uglify-save-license": "^0.4.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "browserslist": [
    "IE 11",
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "last 3 Safari versions",
    "last 3 Edge versions",
    "last 2 iOS version",
    "last 2 ChromeAndroid version",
    "last 2 FirefoxAndroid version",
    "last 2 Samsung versions",
    "last 2 UCAndroid versions"
  ]
}
