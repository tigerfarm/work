{
  "variables": {
    "platform": "ios",
    "product": "auth",
    "sdk-name": "TwilioAuth"
  },
  "major_minor_versions": "sdk/{platform}/{product}/",
  "versions": "sdk/{platform}/{product}/releases/",
  "artifacts": {
    "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.tar.bz2": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.tar.bz2",
    "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.zip": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.zip",
    "sdk/{platform}/{product}/releases/{version}/{sdk-name}.podspec": "sdk/{platform}/{product}/releases/{version}/{sdk-name}.podspec",
    "sdk/{platform}/{product}/releases/{version}/docs/": "sdk/{platform}/{product}/releases/{version}/docs/",
    "sdk/{platform}/{product}/releases/{sdk-name}.json": "sdk/{platform}/{product}/releases/{sdk-name}.json"
  },
  "pin": {
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}-{platform}-{version}.tar.bz2": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.tar.bz2",
    "sdk/{platform}/{product}/v{major}.{minor}/{sdk-name}.podspec": "sdk/{platform}/{product}/releases/{version}/{sdk-name}.podspec",
    "sdk/{platform}/{product}/v{major}.{minor}/docs": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/v{major}.{minor}/docs/index.html": "sdk/{platform}/{product}/releases/{version}/docs"
  },
  "latest": {
    "sdk/{platform}/{product}/latest/twilio-{product}-{platform}.tar.bz2": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.tar.bz2",
    "sdk/{platform}/{product}/latest/twilio-{product}-{platform}.zip": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.zip",
    "sdk/{platform}/{product}/latest/{sdk-name}.podspec": "sdk/{platform}/{product}/releases/{version}/{sdk-name}.podspec",
    "sdk/{platform}/{product}/latest/docs": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/latest/docs/index.html": "sdk/{platform}/{product}/releases/{version}/docs"
  }
}
