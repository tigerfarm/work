{
  "variables": {
    "platform": "js",
    "product": "chat"
  },
  "major_minor_versions": "sdk/{platform}/{product}/",
  "versions": "sdk/{platform}/{product}/releases/",
  "artifacts": {
    "dist/twilio-{product}.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.js",
    "dist/twilio-{product}.min.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.min.js",
    "dist/docs/": "sdk/{platform}/{product}/releases/{version}/docs/"
  },
  "pin": {
    "sdk/{platform}/{product}/v{major}.{minor}/docs": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/v{major}.{minor}/docs/index.html": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/v{major}.{minor}/docs/api": "sdk/{platform}/{product}/releases/{version}/docs/api",
    "sdk/{platform}/{product}/v{major}.{minor}/docs/api/index.html": "sdk/{platform}/{product}/releases/{version}/docs/api",
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.js",
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}.min.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.min.js"
  },
  "latest": {
    "sdk/{platform}/{product}/latest/docs": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/latest/docs/index.html": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/latest/docs/api": "sdk/{platform}/{product}/releases/{version}/docs/api",
    "sdk/{platform}/{product}/latest/docs/api/index.html": "sdk/{platform}/{product}/releases/{version}/docs/api"
  }
}
