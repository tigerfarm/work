{
  "variables": {
    "platform": "android",
    "product": "client"
  },
  "major_minor_versions": "sdk/{platform}/{product}/",
  "versions": "sdk/{platform}/{product}/releases/",
  "artifacts": {
    "dist/{version}/client-release.aar": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.aar",
    "dist/{version}/docs/": "sdk/{platform}/{product}/releases/{version}/docs/"
  },
  "pin": {
    "sdk/{platform}/{product}/v{major}.{minor}/docs": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/v{major}.{minor}/docs/index.html": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}-{platform}.aar": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.aar"
  },
  "latest": {
    "sdk/{platform}/{product}/latest/docs": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/latest/docs/index.html": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/latest/twilio-{product}-{platform}.aar": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.aar"
  }
}
