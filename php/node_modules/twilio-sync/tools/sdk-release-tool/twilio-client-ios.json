{
  "variables": {
    "platform": "ios",
    "product": "client"
  },
  "major_minor_versions": "sdk/{platform}/{product}/",
  "versions": "sdk/{platform}/{product}/releases/",
  "artifacts": {
    "build/twilio-{product}-{platform}.tar.bz2": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.tar.bz2",
    "build/TwilioSDK.podspec": "sdk/{platform}/{product}/releases/{version}/TwilioSDK.podspec"
  },
  "pin": {
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}-{platform}.tar.bz2": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.tar.bz2",
    "sdk/{platform}/{product}/v{major}.{minor}/TwilioSDK.podspec": "sdk/{platform}/{product}/releases/{version}/TwilioSDK.podspec"
  },
  "latest": {
    "sdk/{platform}/{product}/latest/twilio-{product}-{platform}.tar.bz2": "sdk/{platform}/{product}/releases/{version}/twilio-{product}-{platform}-{version}.tar.bz2",
    "sdk/{platform}/{product}/latest/TwilioSDK.podspec": "sdk/{platform}/{product}/releases/{version}/TwilioSDK.podspec"
  }
}
