{
  "variables": {
    "platform": "js",
    "product": "frame-chat"
  },
  "major_minor_versions": "sdk/{platform}/{product}/",
  "versions": "sdk/{platform}/{product}/releases/",
  "artifacts": {
    "dist/package/twilio-{product}.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.js",
    "dist/package/twilio-{product}.min.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.min.js",
    "dist/package/twilio-{product}.bundle.min.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.bundle.min.js",
    "dist/package/twilio-{product}.css": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.css",
    "dist/docs/": "sdk/{platform}/{product}/releases/{version}/docs/"
  },
  "pin": {
    "sdk/{platform}/{product}/v{major}.{minor}/docs": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/v{major}.{minor}/docs/index.html": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.js",
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}.min.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.min.js",
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}.bundle.min.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.bundle.min.js",
    "sdk/{platform}/{product}/v{major}.{minor}/twilio-{product}.css": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.css"
  }
}
