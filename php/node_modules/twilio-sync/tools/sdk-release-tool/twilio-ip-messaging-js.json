{
  "variables": {
    "platform": "js",
    "product": "ip-messaging"
  },
  "major_minor_versions": "sdk/rtc/{platform}/{product}/",
  "versions": "sdk/rtc/{platform}/{product}/releases/",
  "artifacts": {
    "dist/twilio-{product}.js": "sdk/rtc/{platform}/{product}/releases/{version}/twilio-{product}.js",
    "dist/twilio-{product}.min.js": "sdk/rtc/{platform}/{product}/releases/{version}/twilio-{product}.min.js",
    "dist/docs/": "sdk/rtc/{platform}/{product}/releases/{version}/docs/"
  },
  "pin": {
    "sdk/rtc/{platform}/{product}/v{major}.{minor}/docs": "sdk/rtc/{platform}/{product}/releases/{version}/docs",
    "sdk/rtc/{platform}/{product}/v{major}.{minor}/docs/index.html": "sdk/rtc/{platform}/{product}/releases/{version}/docs",
    "sdk/rtc/{platform}/{product}/v{major}.{minor}/docs/api": "sdk/rtc/{platform}/{product}/releases/{version}/docs/api",
    "sdk/rtc/{platform}/{product}/v{major}.{minor}/docs/api/index.html": "sdk/rtc/{platform}/{product}/releases/{version}/docs/api",
    "sdk/rtc/{platform}/{product}/v{major}.{minor}/twilio-{product}.js": "sdk/rtc/{platform}/{product}/releases/{version}/twilio-{product}.js",
    "sdk/rtc/{platform}/{product}/v{major}.{minor}/twilio-{product}.min.js": "sdk/rtc/{platform}/{product}/releases/{version}/twilio-{product}.min.js"
  }
}
