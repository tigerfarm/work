{
  "variables": {
    "platform": "js",
    "product": "taskrouter"
  },
  "major_minor_versions": "sdk/{platform}/{product}/",
  "versions": "sdk/{platform}/{product}/releases/",
  "artifacts": {
    "dist/": "sdk/{platform}/{product}/releases/{version}/"
  },
  "pin": {
    "sdk/{platform}/{product}/v{major}.{minor}/{product}.js": "sdk/{platform}/{product}/releases/{version}/{product}.js",
    "sdk/{platform}/{product}/v{major}.{minor}/{product}.min.js": "sdk/{platform}/{product}/releases/{version}/{product}.min.js",
    "sdk/{platform}/{product}/v{major}/{product}.js": "sdk/{platform}/{product}/releases/{version}/{product}.js",
    "sdk/{platform}/{product}/v{major}/{product}.min.js": "sdk/{platform}/{product}/releases/{version}/{product}.min.js"
  }
}
