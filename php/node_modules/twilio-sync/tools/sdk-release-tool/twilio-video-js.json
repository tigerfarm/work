{
  "variables": {
    "platform": "js",
    "product": "video"
  },
  "major_minor_versions": "sdk/{platform}/{product}/",
  "versions": "sdk/{platform}/{product}/releases/",
  "artifacts": {
    "dist/twilio-{product}.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.js",
    "dist/twilio-{product}.min.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.min.js",
    "dist/docs/": "sdk/{platform}/{product}/releases/{version}/docs/"
  },
  "pin": {
    "sdk/{platform}/{product}/v{major}/twilio-{product}.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.js",
    "sdk/{platform}/{product}/v{major}/twilio-{product}.min.js": "sdk/{platform}/{product}/releases/{version}/twilio-{product}.min.js",
    "sdk/{platform}/{product}/v{major}/docs": "sdk/{platform}/{product}/releases/{version}/docs",
    "sdk/{platform}/{product}/v{major}/docs/index.html": "sdk/{platform}/{product}/releases/{version}/docs"
  }
}
